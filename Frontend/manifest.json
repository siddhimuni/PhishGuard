{
    "name" : "Phised Website Detector",
    "version" : "0.0.1",
    "manifest_version" : 3,
    "action" : {
        "default_popup" : "index.html",
        "default_icon" : "logo.png"
    }, 
    "permissions" : ["activeTab","scripting","tabs","storage"],
    "background": {
        "service_worker": "script.js"
    }
}